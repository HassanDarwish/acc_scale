package jdbc.model;

public class Truck {

	int id;
	int vehicleplate;
	int weight;
	int iddriver;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	 
	public int getVehicleplate() {
		return vehicleplate;
	}
	public void setVehicleplate(int vehicleplate) {
		this.vehicleplate = vehicleplate;
	}
	public int getWeight() {
		return weight;
	}
	public void setWeight(int weight) {
		this.weight = weight;
	}
	public int getIddriver() {
		return iddriver;
	}
	public void setIddriver(int iddriver) {
		this.iddriver = iddriver;
	}
	
	
	
	
}
